  "version": "1.0.0",
  "type": "module",
  "author": "Mario Diaz",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "keywords": [
    "phaser",
    "phaser3",
    "vue",
    "vite"
